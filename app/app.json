{
  "expo": {
    "name": "ConnectVibe",
    "slug": "ConnectVibe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/connect_vive_logo_3",
    "scheme": "social-networking-app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/connect_vive_logo_3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/connect_vive_logo_3.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/connect_vive_logo_3.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
